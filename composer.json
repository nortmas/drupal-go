{
  "name": "nortmas/drupal-go",
  "description": "Drupal 10 project.",
  "version": "0.1.7",
  "require": {
    "php": ">=8.1",
    "digipolisgent/robo-drush": "^5.0",
    "composer/installers": "^2.1",
    "consolidation/robo": "3.0.10",
    "cweagans/composer-patches": "^1.7",
    "drupal/console": "^1.9.10",
    "drupal/core-composer-scaffold": "^10.0.0",
    "drupal/core-recommended": "^10.0.0",
    "drush/drush": "^11.4.0",
    "vlucas/phpdotenv": "^5.4",
    "webflo/drupal-finder": "^1.2"
  },
  "require-dev": {
    "drupal/core-dev": "^10.0.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "discard-changes": true,
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/console-extend-plugin": true,
      "drupal/core-composer-scaffold": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[web-root]/modules/README.txt": false,
        "[web-root]/profiles/README.txt": false,
        "[web-root]/themes/README.txt": false,
        "[web-root]/INSTALL.txt": false,
        "[web-root]/README.txt": false,
        "[web-root]/README.md": false,
        "[web-root]/sites/README.txt": false
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ]
}
