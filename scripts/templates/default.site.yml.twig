self:
  root: '/var/www/html/{{ webRoot }}'
  uri: '{{ project_name }}.docker.localhost:8000'
dev:
  host: '{{ dev.host }}'
  user: '{{ dev.user }}'
  root: '{{ dev.root }}'
  uri: '{{ dev.uri }}'
stage:
  host: '{{ stage.host }}'
  user: '{{ stage.user }}'
  root: '{{ stage.root }}'
  uri: '{{ stage.uri }}'
prod:
  host: '{{ prod.host }}'
  user: '{{ prod.user }}'
  root: '{{ prod.root }}'
  uri: '{{ prod.uri }}'